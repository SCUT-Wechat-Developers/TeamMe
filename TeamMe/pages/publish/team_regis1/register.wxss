/* pages/team_regis/register.wxss */
.teamNameContainer {
    display: flex;
    flex-wrap: wrap;
    box-shadow: 0px 0px 18px 0px rgba(5, 5, 5, 0.15);
    width: 700rpx;
    margin-left: 25rpx;
    border-radius: 10px;
    padding: 10rpx 0;
    position: inherit;
}

.teamNameContainer .title {
    padding-left: 10rpx;
}

.teamInfoContainer {
    border-radius: 100px;
}

.nextStep {
    position: absolute;
    left: 30%;
    top: 100rpx;
    right: 30%;
    bottom: 0;
    margin: auto;
}

