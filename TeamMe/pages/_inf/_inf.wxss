page {
  background-color: #ececec;
}

/* 顶部信息 */
.UCenter-bg {
  background-image: url(https://image.weilanwl.com/color2.0/index.jpg);
  background-size: cover;
  height: 550rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}
/* 头像 */
.UCenter-bg .avatar {
  border-radius: 50%;
}


/*gif图片*/
image[class*="gif-"] {
  border-radius: 6rpx;
  display: block;
}
/*个人基础信息统计*/
.user_content {
  position: relative;
}
.user_content .user_main {
  padding-bottom: 100rpx;
  color: #666;
  position: absolute;
  width: 90%;
  left: 50%;
  transform: translateX(-50%);
  top: -40rpx;
}

/*分割线*/
.divLine {
  margin-left: 35rpx ;
  background: rosybrown; /*背景色为浅灰色*/
  width: 0.1rpx; /*设置宽高*/
  height: auto;
  position: relative; /*调整位置*/
}

/*几个数量：关注数，队伍数，参与数*/
.user_content .user_main .history_wrap {
  height: 150rpx;
  background-color: #fff;
  display: flex;
  display: flex;
  flex-wrap: wrap;
  box-shadow: 0px 0px 18px 0px rgba(5, 5, 5, 0.15);
  width: 680rpx;
  margin-left: 0rpx;
  border-radius: 10px;
  padding: 10rpx ;
}

/*历史足迹*/
.user_content .user_main .history_wrap navigator {
  flex: 1;
  text-align: center;
  padding: 10rpx 0;
}

/*数量的数字部分*/
.user_content .user_main .history_wrap navigator .his_num {
  color: var(--themeColor);
}


.text-apple-black {
  color: #2b2b2b;
}





/*Gary改的*/
.UCenter-bg {
  background-image: url(https://image.weilanwl.com/color2.0/index.jpg);
  background-size: cover;
  height: 550rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}

